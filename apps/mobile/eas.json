{
  "cli": {
    "version": ">= 16.19.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_CONVEX_URL": "https://api.convex.jirak.dev",
        "EXPO_PUBLIC_CONVEX_SITE_URL": "https://convex.jirak.dev"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
